<?xml version="1.0"?>
<launch>
  <!-- start Webots -->
  <arg name="no-gui" default="false," doc="Start Webots with minimal GUI"/>
  <include file="$(find butter_robot_v2)/launch/webots.launch">
    <arg name="mode" value="realtime"/>
    <arg name="no-gui" value="$(arg no-gui)"/>
    <arg name="world" value="$(find butter_robot_v2)/worlds/butter_word.wbt"/>
  </include>

  <arg name="auto-close" default="false" doc="Startup mode"/>
  <!--node name="trainee" pkg="simulacao_webots" type="trainee" required="$(arg auto-close)"/-->
</launch>
